<script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js"></script>
<script>
    WebFont.load({
        google: {
            families: ["Tangerine"]
        },
        active: function () {
            const logo = new Logo({
                canvas,
                backgroundColor: 'blue',
                text: 'canvas',
                width: 200,
                height: 90,
                shape: "rounded",
                fontColor: "white",
                backgroundColor: "#C70039",
                fontFamily: "Tangerine",
                fontSize: 80
            })
            document.body.appendChild(logo.drawLogo())
        }
    });
</script>